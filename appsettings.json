{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DataDome": {
    "ApiKey": "YOUR_DATADOME_API_KEY",
    "EnableBotProtection": true
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": false,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "GeneralRules": [
      {
        "RoutePattern": "*",
        "Period": "1m",
        "Limit": 100
      }
    ]
  },
  "IpRateLimitPolicies": {
    "ClientRules": [
      {
        "ClientId": "dev-client",
        "Rules": [
          {
            "RoutePattern": "*",
            "Period": "1m",
            "Limit": 200
          }
        ]
      }
    ]
  },
  "HttpStatusCode": 429,
  "AllowedHosts": "*"
}